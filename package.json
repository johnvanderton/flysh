{
  "author": "John Van Derton",
  "description": "DOM Document Object Artifact Collector",
  "license": "MIT",
  "name": "flysh",
  "version": "0.2.2",
  "main": "./dist/cjs/index.js",
  "modules": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "directories": {
    "test": "test"
  },
  "bugs": {
    "url": "https://github.com/johnvanderton/flysh/issues"
  },
  "files": ["dist", "version"],
  "scripts": {
    "build": "tsc -b tsconfig.json tsconfig.esm.json tsconfig.types.json",
    "clean": "rimraf dist/*",
    "test [win]": "mocha -r ts-node/register test/**/*.test.ts",
    "test [linux]": "mocha -r ts-node/register test/**/*.test.ts test/**/**/*.test.ts test/src/**/*.test.ts",
    "run": "node dist/cjs/example/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/johnvanderton/flysh.git"
  },
  "dependencies": {
    "@types/node": "^18.0.6",
    "class-transformer": "^0.3.1",
    "es6-shim": "^0.35.6",
    "jquery": "^3.6.1",
    "jsdom": "^20.0.0",
    "npm": "^8.5.2",
    "properties-reader": "^2.2.0",
    "reflect-metadata": "^0.1.13",
    "tslog": "^3.3.2",
    "typedjson": "^1.7.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@types/mocha": "^10.0.0",
    "chai": "^4.3.3",
    "chai-as-promised": "^7.1.1",
    "mocha": "^10.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.5.0",
    "typescript": "^4.2.3"
  },
  "keywords": [
    "crawler",
    "crawler-engine",
    "DOM",
    "HTML",
    "jquery",
    "parser",
    "parser-library",
    "typescript",
    "typescript-library"
  ]
}